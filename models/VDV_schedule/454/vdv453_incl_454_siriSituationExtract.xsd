    <xsd:annotation>
            			
      
      <xsd:documentation>Type for Country identifier.</xsd:documentation>
            		
    
    </xsd:annotation>
        		
    
    <xsd:restriction base="IanaCountryTldEnumeration"/>
        	
  
  </xsd:simpleType>
    	
  
