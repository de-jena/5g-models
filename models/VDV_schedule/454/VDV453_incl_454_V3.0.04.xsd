  <xsd:import namespace="http://www.siri.org.uk/siri" schemaLocation="vdv453_incl_454_siriSituationExtract.xsd"/>
    	
  
  <!--This is the actual dependency which is only to VDV 457 Zaehldaten service-->
    	
  
  <xsd:complexType name="AbbringerFahrtLoeschenType">
        		
    
    <xsd:sequence>
            			
      
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Protokolleintrag" type="ProtokolleintragType"/>
            			
      
      <xsd:element name="ASBID" type="IDType"/>
            			
      
      <xsd:element name="AbbringerInfo" type="AbbringerInfoType"/>
            			
      
      <xsd:element maxOccurs="unbounded" name="FahrtIDExt" type="FahrtIDExtType"/>
            			
      
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="UrsacheText" type="TextMitSprachCodeType"/>
            		
    
    </xsd:sequence>
        		
    
    <xsd:attribute name="Zst" type="xsd:dateTime" use="required"/>
        	
  
  </xsd:complexType>
    	
  
  <xsd:complexType name="AbbringerInfoType">
        		
    
    <xsd:sequence>
            			
      
      <xsd:element name="FahrtID" type="FahrtIDType"/>
            			
      
      <xsd:element minOccurs="0" name="HstSeqZaehler" type="HstSeqZaehlerType"/>
            			
      
      <xsd:element name="LinienID" type="LinienIDType"/>
            			
      
      <xsd:element maxOccurs="unbounded" name="LinienText" type="TextMitSprachCodeType"/>
            			
      
      <xsd:element name="RichtungsID" type="RichtungsIDType"/>
            			
      
      <xsd:element maxOccurs="unbounded" name="RichtungsText" type="TextMitSprachCodeType"/>
            			
      
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="VonRichtungsText" type="TextMitSprachCodeType"/>
            			
      
      <xsd:element name="AbfahrtszeitASBPlan" type="xsd:dateTime"/>
            			
      
      <xsd:element minOccurs="0" name="HaltID" type="HaltIDType"/>
            			
      
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AbfahrtssteigText" type="TextMitSprachCodeType"/>
            			
      
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AbfahrtsSektorenText" type="TextMitSprachCodeType"/>
            			
      
      <xsd:element minOccurs="0" name="AbfahrtsLinienfahrwegID" type="LinienfahrwegIDType"/>
            			
      
      <xsd:element minOccurs="0" name="FahrtInfo" type="FahrtInfoType"/>
            		
    
    </xsd:sequence>
        	
  
  </xsd:complexType>
    	
  
  <xsd:element name="Abbringernachricht" type="AbbringernachrichtType"/>
    	
  
