<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="lm_interface" nsURI="http://inavet.de/datamodels/lm_interface"
    nsPrefix="lm_interface">
  <eClassifiers xsi:type="ecore:EClass" name="PrognosisForLM">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Hauptklasse für Interface FAS -> LM&#xA;presentPosition -> aktuelle Position&#xA;timestamp -> Zeitpunkt der Datenerstellung&#xA;presentElectricalLoad -> laut FAS-Berechnung notwendige Leistung für Antrieb und Nebenverbraucher&#xA;prognosisForPositionAndElectricalLoad ->Liste der Prognose-Positionen und Lastbedarfe"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="presentPosition" lowerBound="1"
        eType="ecore:EClass ../utils/utils.ecore#//Position"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timestamp" lowerBound="1"
        eType="ecore:EDataType ../utils/utils.ecore#//ZonedDateTime"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="presentElectricalLoad"
        lowerBound="1" eType="#//ElectricalLoad"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="prognosisForPositionAndElectricalLoad"
        lowerBound="1" upperBound="-1" eType="#//PrognosisPositionAndElectricalLoad"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PrognosisPositionAndElectricalLoad">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Hilfsklasse für PrognosisForLM&#xA;timestampForPrognosis -> Zeitstempel für die die Prognose gilt&#xA;prognosisPosition -> Prognose-Position basierend auf der optimierten Trajektorie&#xA;electricalLoadPrognosis -> Lastprognose basierend auf der optimierten Trajektorie"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timestampForPrognosis"
        lowerBound="1" eType="ecore:EDataType ../utils/utils.ecore#//ZonedDateTime"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="prognosisPosition" lowerBound="1"
        eType="ecore:EClass ../utils/utils.ecore#//Position"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="electricalLoadPrognosis"
        lowerBound="1" eType="#//ElectricalLoad"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElectricalLoad">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Hilfsklasse für PrognosisForLM + PrognosisPositionAndElectricalLoad&#xA;drivePower -> Leistung für Antrieb&#xA;auxialaryPower -> Leistung für Nebenverbraucher"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="drivePower" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="auxiliaryPower" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ResponseFromLM">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timestamp" lowerBound="1"
        eType="ecore:EDataType ../utils/utils.ecore#//ZonedDateTime"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="states" lowerBound="1"
        upperBound="-1" eType="#//ElectricalGridState"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="consumptions" lowerBound="1"
        upperBound="-1" eType="#//TramPowerConsumption"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TramPowerConsumption">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="vehicleId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="currentPowerConsumption"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElectricalGridState">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Status des elktischen Netzes, bestehend aus einzelnen Abschnitten. Der Startzeitppunkt erlaubt die zeitliche Einordnung. Ein Status kann für mehrere Sekunden anhalten, was von der angegebenen duration. abhängt "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="startTimestamp" lowerBound="1"
        eType="ecore:EDataType ../utils/utils.ecore#//ZonedDateTime"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="durationSeconds" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sections" lowerBound="1"
        upperBound="-1" eType="#//ElectricalGridSectionState"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElectricalGridSectionState">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="sectionId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalAvailablePower" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalUsedPower" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="trams" lowerBound="1" upperBound="-1"
        eType="#//TramPowerMapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TramPowerMapping">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="vehicleId" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="powerReserve" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Wenn negativ, sollte die von der Bahn in diesem Zeitabschnitt verbrauchte Energie um den gegebenen Betrag reduziert werden."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
</ecore:EPackage>
